<% layout('/layouts/boilerplate') %> 

    <h1>All Campgrounds</h1>
    <div>
        <a href="/campgrounds/new">Add new campground</a>
    </div>
    <ul>
        <% for (let camp of allCamps) { %>
            <a href="/campgrounds/<%= camp._id %>"><li><%= camp.title %> </li></a>
            <% } %>  
    </ul>
    



